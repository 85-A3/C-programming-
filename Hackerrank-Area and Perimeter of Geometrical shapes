#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int r,a,l,w;
    scanf("%d %d %d %d",&r,&a,&l,&w);
    float a0,p0,al,pl,a2,p2;
    const float pi = 3.1416;
    a0 = pi * r * r;
    p0 = 2 * pi *r;
    al = a * a;
    pl = 4 * a;
    a2 = l * w;
    p2 =2*(l + w);
    printf("Circle: %.2f %.2f\nSquare: %.2f %.2f\nRectangle: %.2f %.2f",a0,p0,al,pl,a2,p2);
    return 0;
}
