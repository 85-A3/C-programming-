#include<stdio.h>
int main(){
    int arr[10][10],rows,cols,i,j;
    printf("Enter rowsnand columns:");
    scanf("%d",&rows,cols);
    printf("Enter elements:\n");
    for(i=0;i<rows;i++){
        for(j=0;j<cols;j++){
            scanf("%d",&arr[i][j]);
        }
    }
    printf("Matrix is :\n");
    for(i=0;i<rows;i++){
        for(j=0;j<cols;j++){
            printf("%d",arr[i][j]);
        }
        printf("\n");
    }
    return 0;
}
