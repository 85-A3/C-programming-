#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int N, key;
    
    
    if (scanf("%d", &N) != 1) {
        return 1;
    }
    
    int array[N];
    
   
    for (int i = 0; i < N; i++) {
        if (scanf("%d", &array[i]) != 1) {
            return 1;
        }
    }
    
    
    if (scanf("%d", &key) != 1) {
        return 1;
    }
    
    int count = 0;
    
    
    for (int i = 0; i < N; i++) {
        if (array[i] == key) {
            count++;
        }
    }
    
    
    printf("%d\n", count);
    
    return 0;
}
